{
  "name": "termo",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build --progress=false --prod --aot",
    "get-stats": "npm run build -- --stats-json",
    "bundle-analyze": "npx webpack-bundle-analyzer dist/stats.json",
    "test": "jest --ci",
    "test-watch": "jest --watchAll",
    "deploy": "npm run test && npm run build",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "ngcc": "ngcc",
    "postinstall": "ngcc --properties es2015"
  },
  "private": true,
  "jest": {
    "preset": "jest-preset-angular",
    "setupFilesAfterEnv": [
      "<rootDir>/src/setupJest.ts"
    ],
    "transformIgnorePatterns": [
      "<rootDir>/node_modules/(?!(lodash-es|date-fns|@material)/)"
    ],
    "testPathIgnorePatterns": [
      "<rootDir>/src/test.ts"
    ]
  },
  "dependencies": {
    "@angular/animations": "^11.2.14",
    "@angular/cdk": "^11.2.13",
    "@angular/common": "^11.2.14",
    "@angular/compiler": "^11.2.14",
    "@angular/core": "^11.2.14",
    "@angular/forms": "^11.2.14",
    "@angular/platform-browser": "^11.2.14",
    "@angular/platform-browser-dynamic": "^11.2.14",
    "@angular/router": "^11.2.14",
    "@material/snackbar": "^13.0.0",
    "@ngrx/effects": "11.1.1",
    "@ngrx/entity": "11.1.1",
    "@ngrx/store": "11.1.1",
    "@ngrx/store-devtools": "11.1.1",
    "@sentry/browser": "^6.19.6",
    "@swimlane/ngx-charts": "^18.0.0",
    "backoff-rxjs": "^6.5.7",
    "date-fns": "^2.28.0",
    "lodash": "^4.17.21",
    "lodash-es": "^4.17.21",
    "normalize.css": "^8.0.0",
    "paho-mqtt": "^1.1.0",
    "rxjs": "^6.5.5",
    "tslib": "^2.0.0",
    "zone.js": "~0.10.2"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.1102.19",
    "@angular/cli": "^11.2.19",
    "@angular/compiler-cli": "^11.2.14",
    "@angular/language-service": "^11.2.14",
    "@ngrx/schematics": "11.1.1",
    "@types/jest": "^24.0.18",
    "@types/lodash-es": "^4.17.6",
    "@types/node": "^12.11.1",
    "codelyzer": "^6.0.0",
    "envsub": "^4.0.7",
    "jest": "^24.0.0",
    "jest-marbles": "^2.0.0",
    "jest-preset-angular": "^7.0.0",
    "ts-node": "~5.0.1",
    "tslint": "~6.1.0",
    "typescript": "4.0.8",
    "vrsource-tslint-rules": "^5.8.2",
    "webpack-bundle-analyzer": "^3.3.2"
  }
}
