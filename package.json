{
  "name": "termo",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build --progress=false --configuration production --aot",
    "get-stats": "npm run build -- --stats-json",
    "bundle-analyze": "npx webpack-bundle-analyzer dist/stats.json",
    "test": "jest",
    "test:ci": "jest --ci",
    "test:watch": "jest --watchAll",
    "deploy": "npm run test:ci && npm run build",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "ngcc": "ngcc",
    "postinstall": "ngcc --properties es2015 main"
  },
  "private": true,
  "jest": {
    "globals": {
      "ts-jest": {
        "tsconfig": "<rootDir>/src/tsconfig.spec.json"
      }
    },
    "preset": "jest-preset-angular",
    "setupFilesAfterEnv": [
      "<rootDir>/src/setupJest.ts"
    ],
    "globalSetup": "jest-preset-angular/global-setup",
    "transformIgnorePatterns": [
      "<rootDir>/node_modules/(?!(lodash-es|date-fns|@material)/)"
    ],
    "testPathIgnorePatterns": [
      "<rootDir>/src/test.ts"
    ],
    "moduleNameMapper": {
      "^environments/(.*)$": "<rootDir>/src/environments/$1"
    }
  },
  "dependencies": {
    "@angular/animations": "^13.4.0",
    "@angular/cdk": "^13.3.9",
    "@angular/common": "^13.4.0",
    "@angular/compiler": "^13.4.0",
    "@angular/core": "^13.4.0",
    "@angular/forms": "^13.4.0",
    "@angular/platform-browser": "^13.4.0",
    "@angular/platform-browser-dynamic": "^13.4.0",
    "@angular/router": "^13.4.0",
    "@material/snackbar": "^13.0.0",
    "@ngrx/effects": "13.1.0",
    "@ngrx/entity": "13.1.0",
    "@ngrx/store": "13.1.0",
    "@ngrx/store-devtools": "13.1.0",
    "@sentry/browser": "^6.19.6",
    "@swimlane/ngx-charts": "^20.1.0",
    "backoff-rxjs": "^7.0.0",
    "date-fns": "^2.28.0",
    "lodash": "^4.17.21",
    "lodash-es": "^4.17.21",
    "normalize.css": "^8.0.0",
    "paho-mqtt": "^1.1.0",
    "rxjs": "^7.8.0",
    "tslib": "^2.0.0",
    "zone.js": "~0.11.4"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~13.3.11",
    "@angular/cli": "^13.3.11",
    "@angular/compiler-cli": "^13.4.0",
    "@ngrx/schematics": "13.1.0",
    "@types/jest": "^28",
    "@types/lodash-es": "^4.17.6",
    "@types/node": "^12.11.1",
    "codelyzer": "^6.0.0",
    "envsub": "^4.0.7",
    "jest": "^28",
    "jest-marbles": "^3.0.3",
    "jest-preset-angular": "^12",
    "ts-node": "~5.0.1",
    "tslint": "~6.1.0",
    "typescript": "4.6.4",
    "vrsource-tslint-rules": "^5.8.2",
    "webpack-bundle-analyzer": "^3.3.2"
  }
}