{
  "name": "termo",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build --progress=false --configuration production --aot",
    "get-stats": "npm run build -- --stats-json",
    "bundle-analyze": "npx webpack-bundle-analyzer dist/stats.json",
    "envsub": "envsub",
    "sentry-release": "sh ./bin/sentry-release.sh",
    "deploy": "npm run lint && npm run build && npm run post-deploy",
    "post-deploy": "envsub ./dist/index.html && envsub netlify.toml.template netlify.toml && npm run sentry-release",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "ngcc": "ngcc",
    "postinstall": "ngcc --properties es2015 main"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^15.2.6",
    "@angular/cdk": "^14.2.7",
    "@angular/common": "^15.2.6",
    "@angular/compiler": "^15.2.6",
    "@angular/core": "^15.2.6",
    "@angular/forms": "^15.2.6",
    "@angular/platform-browser": "^15.2.6",
    "@angular/platform-browser-dynamic": "^15.2.6",
    "@angular/router": "^15.2.6",
    "@material/snackbar": "^13.0.0",
    "@ngrx/effects": "14.3.3",
    "@ngrx/entity": "14.3.3",
    "@ngrx/store": "14.3.3",
    "@ngrx/store-devtools": "14.3.3",
    "@sentry/browser": "^6.19.6",
    "@swimlane/ngx-charts": "^20.1.2",
    "backoff-rxjs": "^7.0.0",
    "d3-color-2-fix": "2.0.1",
    "date-fns": "^2.28.0",
    "lodash": "^4.17.21",
    "lodash-es": "^4.17.21",
    "normalize.css": "^8.0.0",
    "paho-mqtt": "^1.1.0",
    "rxjs": "^7.8.0",
    "tslib": "^2.0.0",
    "zone.js": "~0.11.4"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^15.2.5",
    "@angular-eslint/builder": "15.2.1",
    "@angular-eslint/eslint-plugin": "15.2.1",
    "@angular-eslint/eslint-plugin-template": "15.2.1",
    "@angular-eslint/schematics": "15.2.1",
    "@angular-eslint/template-parser": "15.2.1",
    "@angular/cli": "^15.2.5",
    "@angular/compiler-cli": "^15.2.6",
    "@ngrx/schematics": "14.3.3",
    "@sentry/cli": "^2.17.0",
    "@types/lodash-es": "^4.17.6",
    "@types/node": "^12.11.1",
    "@typescript-eslint/eslint-plugin": "^5.43.0",
    "@typescript-eslint/parser": "^5.43.0",
    "envsub": "^4.1.0",
    "eslint": "^8.28.0",
    "ts-node": "~5.0.1",
    "typescript": "4.9.5",
    "webpack-bundle-analyzer": "^4.8.0"
  },
  "overrides": {
    "d3-color": "npm:d3-color-2-fix@2.0.1"
  }
}
