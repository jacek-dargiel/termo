{
  "name": "termo",
  "version": "0.0.0",
  "type": "module",
  "engines": {
    "node": "22.x"
  },
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build --progress=false --configuration production --aot",
    "get-stats": "npm run build -- --stats-json",
    "bundle-analyze": "npx webpack-bundle-analyzer dist/stats.json",
    "envsub": "envsub",
    "sentry-release": "sh ./bin/sentry-release.sh",
    "e2e-setup": "playwright install",
    "deploy": "npm run lint && npm run test && npm run e2e-setup && npm run e2e && npm run build && npm run post-deploy",
    "post-deploy": "envsub ./dist/browser/index.html && envsub netlify.toml.template netlify.toml && npm run sentry-release",
    "lint": "ng lint",
    "test": "jest --silent --maxWorkers=2",
    "test:watch": "jest --watch",
    "e2e": "ng e2e"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^19.2.14",
    "@angular/cdk": "^19.2.19",
    "@angular/common": "^19.2.14",
    "@angular/compiler": "^19.2.14",
    "@angular/core": "^19.2.14",
    "@angular/forms": "^19.2.14",
    "@angular/platform-browser": "^19.2.14",
    "@angular/platform-browser-dynamic": "^19.2.14",
    "@angular/router": "^19.2.14",
    "@material/snackbar": "^14.0.0",
    "@ngrx/effects": "19.2.1",
    "@ngrx/entity": "19.2.1",
    "@ngrx/operators": "^19.2.1",
    "@ngrx/store": "19.2.1",
    "@ngrx/store-devtools": "19.2.1",
    "@sentry/browser": "^7.47.0",
    "@swimlane/ngx-charts": "^23.0.1",
    "backoff-rxjs": "^7.0.0",
    "date-fns": "^2.28.3",
    "normalize.css": "^8.0.0",
    "rxjs": "^7.8.0",
    "tslib": "^2.0.0",
    "zone.js": "~0.15.1"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^19.2.15",
    "@angular-eslint/builder": "19.8.1",
    "@angular-eslint/eslint-plugin": "19.8.1",
    "@angular-eslint/eslint-plugin-template": "19.8.1",
    "@angular-eslint/schematics": "19.8.1",
    "@angular-eslint/template-parser": "19.8.1",
    "@angular/cli": "^19.2.15",
    "@angular/compiler-cli": "^19.2.14",
    "@ngrx/schematics": "19.2.1",
    "@playwright/test": "1.55.1",
    "@sentry/cli": "^2.17.0",
    "@types/jest": "^30.0.0",
    "@types/node": "^22",
    "@typescript-eslint/eslint-plugin": "^8.43.0",
    "@typescript-eslint/parser": "^8.43.0",
    "envsub": "^4.1.0",
    "eslint": "^9.35.0",
    "jest": "^30.1.1",
    "jest-marbles": "^3.1.1",
    "jest-preset-angular": "^15.0.0",
    "ng-mocks": "^14.13.5",
    "playwright-ng-schematics": "^2.0.3",
    "ts-node": "~5.0.1",
    "typescript": "5.8.3",
    "webpack-bundle-analyzer": "^4.8.0"
  },
  "overrides": {
    "@angular-devkit/build-angular": {
      "jest": "^30.0",
      "jest-environment-jsdom": "^30.0"
    }
  }
}
