{
  "name": "termo",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build --progress=false --prod --aot",
    "postbuild": "node ./node_modules/envsub/bin/envsub.js ./netlify.toml.template ./netlify.toml",
    "get-stats": "yarn run build --stats-json",
    "bundle-analyze": "npx webpack-bundle-analyzer dist/stats.json",
    "test": "jest --ci",
    "test-watch": "jest --watchAll",
    "deploy": "yarn run test && yarn run build",
    "lint": "ng lint",
    "e2e": "ng e2e"
  },
  "private": true,
  "jest": {
    "preset": "jest-preset-angular",
    "setupTestFrameworkScriptFile": "<rootDir>/src/setupJest.ts",
    "transformIgnorePatterns": [
      "<rootDir>/node_modules/(?!(lodash-es|date-fns|@material)/)"
    ]
  },
  "dependencies": {
    "@angular/animations": "^7.1.4",
    "@angular/common": "^7.1.4",
    "@angular/compiler": "^7.1.4",
    "@angular/core": "^7.1.4",
    "@angular/forms": "^7.1.4",
    "@angular/http": "^7.1.4",
    "@angular/platform-browser": "^7.1.4",
    "@angular/platform-browser-dynamic": "^7.1.4",
    "@angular/router": "^7.1.4",
    "@material/snackbar": "^0.38.0",
    "@ngrx/effects": "7.0.0",
    "@ngrx/entity": "7.0.0",
    "@ngrx/store": "7.0.0",
    "@ngrx/store-devtools": "7.0.0",
    "@swimlane/ngx-charts": "10",
    "core-js": "^2.5.4",
    "date-fns": "^2.0.0-alpha.9",
    "lodash": "^4.17.10",
    "lodash-es": "^4.17.11",
    "mqtt": "^2.18.8",
    "normalize.css": "^8.0.0",
    "rxjs": "^6.3.3",
    "tslib": "^1.9.0",
    "zone.js": "^0.8.26"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.11.0",
    "@angular/cli": "^7.1.4",
    "@angular/compiler-cli": "^7.1.4",
    "@angular/language-service": "^7.1.4",
    "@ngrx/schematics": "7.0.0",
    "@types/lodash-es": "^4.17.1",
    "@types/material__snackbar": "^0.35.0",
    "@types/node": "~8.9.4",
    "codelyzer": "4.5",
    "envsub": "^3.1.0",
    "jest": "^23.6.0",
    "jest-marbles": "^2.0.0",
    "jest-preset-angular": "^6.0.0",
    "ts-node": "~5.0.1",
    "tslint": "~5.9.1",
    "typescript": "3.1.6",
    "vrsource-tslint-rules": "^5.8.2",
    "webpack-bundle-analyzer": "^2.13.1"
  }
}
